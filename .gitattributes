*          text=auto

# Docker
*.dockerignore text
Dockerfile text

# Dotnet
*.cs       text diff=csharp
*.sln      text eol=crlf
*.csproj   text eol=crlf
*.dll      binary

# PowerShell
*.ps1      text eol=crlf
*.ps1x     text eol=crlf
*.psm1     text eol=crlf
*.psd1     text eol=crlf
*.ps1xml   text eol=crlf
*.pssc     text eol=crlf
*.psrc     text eol=crlf
*.cdxml    text eol=crlf

# Python
*.pxd      text diff=python
*.py       text diff=python
*.py3      text diff=python
*.pyw      text diff=python
*.pyx      text diff=python
*.pyz      text diff=python
*.db       binary filter=lfs diff=lfs merge=lfs binary
*.p        binary filter=lfs diff=lfs merge=lfs binary
*.pkl      binary filter=lfs diff=lfs merge=lfs binary
*.pickle   binary filter=lfs diff=lfs merge=lfs binary
*.pyc      binary filter=lfs diff=lfs merge=lfs binary
*.pyd      binary filter=lfs diff=lfs merge=lfs binary
*.pyo      binary filter=lfs diff=lfs merge=lfs binary

# R
*.Rdata    binary
*.RData    binary
*.rda      binary
*.Rmd      text
*.Rd       text
*.rdb      filter=lfs diff=lfs merge=lfs binary
*.rds      filter=lfs diff=lfs merge=lfs binary
*.Rdx      filter=lfs diff=lfs merge=lfs binary
*.R        text diff=rstats
*.Rproj    text

# Jupyter notebook
*.ipynb    text

# Web
*.css      text diff=css
*.htm      text diff=html
*.html     text diff=html
*.md       text diff=markdown linguist-detectable

# Data
*.csv      filter=lfs diff=lfs merge=lfs eol=crlf
*.xls*     filter=lfs diff=lfs merge=lfs
*.hdf5     filter=lfs diff=lfs merge=lfs

# Images
*.gif      filter=lfs diff=lfs merge=lfs -text
*.ico      filter=lfs diff=lfs merge=lfs -text
*.jpg      filter=lfs diff=lfs merge=lfs -text
*.png      filter=lfs diff=lfs merge=lfs -text
*.psd      filter=lfs diff=lfs merge=lfs -text
*.webp     filter=lfs diff=lfs merge=lfs -text

# Documents
*.pdf      filter=lfs diff=lfs merge=lfs -text
*.doc*     filter=lfs diff=lfs merge=lfs -text

# Archives
*.7z       filter=lfs diff=lfs merge=lfs -text
*.br       filter=lfs diff=lfs merge=lfs -text
*.gz       filter=lfs diff=lfs merge=lfs -text
*.tar      filter=lfs diff=lfs merge=lfs -text
*.zip      filter=lfs diff=lfs merge=lfs -text

# Other
*.exe      filter=lfs diff=lfs merge=lfs -text
